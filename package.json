{
  "name": "facebook-event-scraper",
  "version": "0.1.3",
  "type": "module",
  "description": "A slim module for scraping Facebook event data in milliseconds.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "ts-node src/index.ts",
    "server": "node server.js",
    "server:dev": "DRY_RUN=true node server.js",
    "add-event": "node add_event.js",
    "import-event": "node import_event.js",
    "import-timetable": "node import_timetable.js",
    "clashfinder": "node get_data/get_clashfinder_timetable.js",
    "scrape-promoters": "node scrape_promoter_events.js",
    "scrape-promoters:dry": "node scrape_promoter_events.js --dry-run",
    "test-festival": "node tests/test_festival_days_detection.js",
    "prepare": "husky install",
    "test": "jest",
    "test:unit": "npm test -- --testPathIgnorePatterns src/tests/e2e.test.ts",
    "test:e2e": "npm test -- --testPathPattern src/tests/e2e.test.ts",
    "test:update-snapshot": "npm run test:e2e -- --updateSnapshot",
    "prepack": "npm run test:unit && rm -rf dist && npm run build",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/francescov1/facebook-event-scraper.git"
  },
  "keywords": [
    "facebook",
    "event",
    "scrape",
    "scraper",
    "webscraper",
    "axios",
    "slim",
    "fast",
    "events",
    "facebook-events"
  ],
  "license": "MIT",
  "author": "Francesco Virga @francescov1",
  "bugs": {
    "url": "https://github.com/francescov1/facebook-event-scraper/issues"
  },
  "files": [
    "/dist"
  ],
  "homepage": "https://github.com/francescov1/facebook-event-scraper#readme",
  "dependencies": {
    "@supabase/supabase-js": "^2.49.4",
    "aws4fetch": "^1.0.20",
    "axios": "^1.8.4",
    "cli-progress": "^3.12.0",
    "csv-parse": "^6.1.0",
    "dotenv": "^16.5.0",
    "facebook-event-scraper": "^0.2.6",
    "i18n-iso-countries": "^7.14.0",
    "luxon": "^3.6.1",
    "node-fetch": "^3.3.2",
    "node-geocoder": "^4.4.1",
    "openai": "^4.90.0",
    "string-similarity": "^4.0.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "bounce-in-style": "^0.0.40",
    "eslint": "^7.32.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3"
  }
}
